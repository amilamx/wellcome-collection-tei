<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="https://raw.githubusercontent.com/bodleian/consolidated-tei-schema/master/msdesc.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>

<!-- Template created 16.11.2022 -->

<TEI xmlns="http://www.tei-c.org/ns/1.0" xml:id="">
    <!-- Give xml:id in the  format  "" -the file name should be identical -->

    <teiHeader>
    
    <fileDesc>
        
        <titleStmt>
            <title><!-- Give title in the format ""  --></title>
            <title type="collection">Wellcome Collection</title>
            <respStmt xml:id="LS">
                <resp when="2020-10-05">Template</resp>
                <persName>Lara Salha</persName>
            </respStmt>
            <respStmt xml:id="">
                <resp when="">TEI Encoder</resp>
                <persName><!-- Insert name --></persName>
            </respStmt>
            <!-- Add another <respStmt> for every resp -->
        </titleStmt>
        
        <publicationStmt>
            <publisher>Wellcome Collection</publisher>
            <idno>UkLW</idno>
            <address>
                <orgName type="institution">Wellcome Collection</orgName>
                <street>215 Euston Rd</street>
                <settlement>London</settlement>
                <postCode>NW1 2BE</postCode>
            </address>
            <distributor>
                <email>library@wellcome.org</email>
            </distributor>
            <idno type="msID"><!-- Give the shelfmark of the manuscript --></idno>
            <idno type="catalogue"><!-- Leave blank --></idno>
            <availability>
                <licence><!-- Licensing statement to come in late-stage editing --></licence>
            </availability>
        </publicationStmt>
        
        <sourceDesc>
            
            <msDesc>
                
                <msIdentifier>
                    <country>United Kingdom</country>
                    <settlement>London</settlement>
                    <institution>Wellcome Collection</institution>
                    <repository>Wellcome Library</repository>
                    <idno type="shelfmark"></idno>
                    <altIdentifier type="Sierra">
                        <idno>
                            <!-- Insert Sierra b number here -->
                        </idno>
                    </altIdentifier>
                    <altIdentifier type="accession">
                        <idno>
                         <!-- Insert accession number here -->
                        </idno>
                    </altIdentifier>
                </msIdentifier>
                
                <msContents>
                  
                    <summary><!-- Any top-level description of the manuscript as a whole if possible --></summary>
                    <textLang mainLang="" source="IANA"><!-- If no language is cited in donor info or can't be easily identified then leave blank --></textLang>
                                       
                    <msItem n="" xml:id="">
                        <author key="">
                            <!-- Insert viaf code as attribute value if available in key --> 
                            <persName> <!-- if available, and enter full author name here --> </persName></author>
                        <title><!--  Enter title here --></title>
                        <note>
                            <p>At this early stage of cataloguing, any language noted is not guaranteed to be the only language in the whole manuscript.</p>
                        </note>
                      
                        
                    </msItem>
                    
                    
                </msContents>
                
                <physDesc>
                    <!-- Insert and delete as needed -->
                    <objectDesc form=""> <!-- Insert type i.e codex -->
                       
                        <supportDesc material=""> <!-- can material be identified? paper? parchment? -->
                                                    
                            <extent><!-- Insert details on extent -->
                                <dimensions unit="mm" ><!-- Add if available -->
                                    <dim type="diameter"></dim>
                                    <dim type="length"></dim>
                                </dimensions>
                            </extent>
                         
                            <condition>
                                <!-- Use to record information on the manuscript's condition according to inventory schema, A-D -->
                            </condition>
                            
                        </supportDesc>                        
                         
                    </objectDesc>

                    
                </physDesc>
                
                <history>
                    <origin>
                        <origPlace>
                            <country><!-- insert --></country>
                        </origPlace>
                        <origDate calendar=""><!-- insert --></origDate>
                        <!-- use additional origPlace and origDate elements if necessary -->
                    </origin>
                    <provenance notBefore="" notAfter=""><!-- insert, otherwise delete --></provenance>
                    <!-- use additional provenance elements if necessary -->
                    <acquisition><!-- insert --></acquisition>
                </history>
                
                <additional>
                    <adminInfo>
                        <availability status=""><p> <!-- Insert if relevant --></p>
                        </availability> 
                    </adminInfo>
                
                </additional>
            
            </msDesc>
        
        </sourceDesc>
    
    </fileDesc>
        
        <!-- Use the following to record subject headings, if needed -->
        <encodingDesc>
            <classDecl>
                <taxonomy xml:id="LCSH">
                    <bibl>
                        <ref target="http://id.loc.gov/authorities/about.html#lcsh">Library of Congress Subject Headings</ref>
                    </bibl>
                </taxonomy>
            </classDecl>
        </encodingDesc>
        <profileDesc>
            <textClass>
                <keywords scheme="#LCSH">
                    <list>
                        <item facs="">
                            <term ref="subject_"></term>
                        </item>
                    </list>
                </keywords>
            </textClass>
        </profileDesc>    
    
        <revisionDesc>
        <change when=""><!-- Make an entry to record your starting work on the file and then a new <change> for later edits, following the below format --></change>
        <change when="2022-11-16" who="#LS">Created template for Minimum Viable Record TEI files.</change>
        </revisionDesc>

    </teiHeader>    

    <facsimile>
        <surface></surface>

    </facsimile>    

    <text>
    <body>
        <p></p>
        <!-- For future full transcription -->
    </body>

    </text>    

</TEI>