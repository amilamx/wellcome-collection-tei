<!-- Template created 16.11.2022 -->

<TEI xmlns="http://www.tei-c.org/ns/1.0" xml:id="">
    <!-- Give xml:id in the  format  "" -the file name should be identical -->

    <teiHeader>
    
    <fileDesc>
        
        <titleStmt>
            <title type="collection">Wellcome Collection</title>
            <respStmt xml:id="AP">
                <resp when="2020-10-05">Template</resp>
                <persName>Adrian Plau</persName>
            </respStmt>
            <respStmt xml:id="LS">
                <resp when="2022-11-16">Template editor</resp>
                <persName>Lara Salha</persName>
            </respStmt>
            <respStmt xml:id="SC">
                <resp when="2023-01-11">Template editor</resp>
                <persName>Stephanie Cornwell</persName>
            </respStmt>
            <respStmt xml:id="">
            <!-- Insert initials -->
                <resp when="">TEI Encoder</resp>
                <persName><!-- Insert name --></persName>
            </respStmt>
            <!-- Add another <respStmt> for every person responsible for editing the record in future -->
        </titleStmt>
        
        <publicationStmt>
            <publisher>Wellcome Collection</publisher>
            <idno>UkLW</idno>
            <address>
                <orgName type="institution">Wellcome Collection</orgName>
                <street>215 Euston Rd</street>
                <settlement>London</settlement>
                <postCode>NW1 2BE</postCode>
            </address>
            <distributor>
                <email>library@wellcome.org</email>
            </distributor>
            <idno type="msID"><!-- Give the shelfmark/reference number of the manuscript --></idno>
            <idno type="catalogue"><!-- Leave blank --></idno>
            <availability>
                <licence><!-- Licensing statement to come in late-stage editing --></licence>
            </availability>
        </publicationStmt>
        
        <sourceDesc>
            
            <msDesc>
                
                <msIdentifier>
                    <country>United Kingdom</country>
                    <settlement>London</settlement>
                    <institution>Wellcome Collection</institution>
                    <repository>Wellcome Library</repository>
                    <idno type="shelfmark"><!-- Give the shelfmark/reference number of the manuscript --></idno>
                    <altIdentifier type="Sierra">
                        <idno><!-- Insert Sierra b number here --></idno>                                               
                    </altIdentifier>
                    <altIdentifier type="accession">
                        <idno><!-- Insert accession number here --></idno>
                    </altIdentifier>
                </msIdentifier>
                
                <msContents>
                  
                    <summary><!-- Any top-level description of the manuscript as a whole if possible --></summary>
                    <textLang mainLang="" source="IANA"><!-- If no language is cited in donor info or can't be easily identified then leave blank --></textLang>
                                       
                    <msItem n="" xml:id="">
                    <!-- Add 1 to the n value and xml:id for each item described ie. first n="1" xml:id="MS.0000_1", second n="2" xml:id="MS.0000_2" --> 
                        <author key="person_" source="VIAF">
                            <!-- Insert viaf code as attribute value if available in key --> 
                            <persName> <!-- if available, and enter full author name here --> </persName></author>
                        <title><!--  Enter title here --></title>
                        <note>
                            <p>At this early stage of cataloguing, any language noted is not guaranteed to be the only language in the whole manuscript.</p>
                        </note>         
                    </msItem>
                                        
                </msContents>
                
                <physDesc>                
                    <objectDesc form=""> <!-- Insert type i.e codex or folio -->            
                        <supportDesc material=""> <!-- Insert if material can be easily identified i.e. paper or parchment -->
                                
                            <extent><!-- Insert details on extent -->
                                <dimensions unit="mm" ><!-- Add if available -->
                                    <dim type="diameter"></dim>
                                    <dim type="length"></dim>
                                </dimensions>
                            </extent>
                         
                            <condition><!-- Use to record information on the manuscript's condition according to inventory schema, A-D --></condition>
                            
                        </supportDesc>                                             
                    </objectDesc>                 
                </physDesc>
                
                <history>
                    <origin>
                        <origPlace>
                            <country><!-- insert --></country>
                        </origPlace>
                        <origDate calendar="gregorian"><!-- insert --></origDate>
                    </origin>
                    <provenance notBefore="" notAfter=""><!-- insert prior owners where relevant --></provenance>
                    <acquisition><!-- insert acquisition date and method --></acquisition>
                </history>
            
            </msDesc>
        
        </sourceDesc>
    
    </fileDesc>
        
        
        <encodingDesc>
            <classDecl>
                <taxonomy xml:id="LCSH">
                    <bibl>
                        <ref target="http://id.loc.gov/authorities/about.html#lcsh">Library of Congress Subject Headings</ref>
                    </bibl>
                </taxonomy>
            </classDecl>
        </encodingDesc>
        <profileDesc>
            <textClass>
                <keywords scheme="#LCSH">
                    <list>
                        <item>
                            <term ref="subject_"><!-- Insert sh number and term here, delete if not used. --></term>
                        </item>
                    </list>
                </keywords>
            </textClass>
        </profileDesc>    
    
        <revisionDesc>
        <change when="" who="">Populated template</change>
        <change when="2022-11-16" who="#LS">Created template for Minimum Viable Record TEI files.</change>
        <change when="2023-01-11" who="#SC">Edited comments and removed unnecessary spaces.</change>
        <!-- Any further major revisions made to this record in future should be recorded in this same format -->
        </revisionDesc>

    </teiHeader>    

    <facsimile>
        <surface></surface>

    </facsimile>    

    <text>
    <body>
        <p></p>
        <!-- For future full transcription -->
    </body>

    </text>    

</TEI>