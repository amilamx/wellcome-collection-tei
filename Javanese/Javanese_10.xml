                <msContents>
                  
                    <summary>Fragments, mainly on mysticism and divination.</summary>
                   
                    <textLang mainLang="jv">Javanese</textLang>
                    
                    <msItem n="1" xml:id="Javanese_10_A">
                        <locus>Ff. 1r.-2v.</locus>
                        <note>Letters of the Javanese syllabary associated with the compass directions, etc. (Bound upside down).</note>
                        
                    </msItem>
                    
                    <msItem n="2" xml:id="Javanese_10_B">
                        <locus>Ff. 3r.-v.</locus>
                        
                        <incipit>
                            Begins punika lampah ing dina pepitu
                        </incipit>
                        
                    </msItem>
                    
                    <msItem n="3" xml:id="Javanese_10_C">
                        <locus> Ff. 4r.-v.</locus>
                        
                        <incipit>
                            Begins punika esmu kasiyan saking <persName>Kanjeng Kyai Nur Muhamad</persName>
                        </incipit>
                        
                        <explicit>
                            Incomplete at end.
                        </explicit>
                        
                    </msItem>
                    
                    <msItem n="4" xml:id="Javanese_10_D">
                        <locus> Ff. 5r.-6v. Ff. 5v.-6r. blank. </locus>
                        <note>Fragment concerning horses named <persName>Suryakanda</persName>, <persName>Wisnumurti</persName>, etc.</note>
                        
                    </msItem>
                    
                    <msItem n="5" xml:id="Javanese_10_E">
                        <locus>Ff. 7r.-12r.</locus>
                        <incipit>Begins iki sampurnani[ng]puji... iki dikirekang walung prakara.</incipit>
                        
                    </msItem>
                    
                    <msItem n="6" xml:id="Javanese_10_F">
                        <locus>Ff. 12v.-13r. </locus>
                        <note>Divination table.</note>
                        
                        
                    </msItem>
                    
                    <msItem n="7" xml:id="Javanese_10_G">
                        <locus>Ff. 14r.-21v.</locus>
                        <incipit>Begins punika dongane wong.</incipit>
                        
                    </msItem>
                    
                    <msItem n="8" xml:id="Javanese_10_H">
                        <locus>Ff. 21v-22v.</locus>
                        <note>Fragment concerning the Javanese five-day week.</note>
                        
                    </msItem>
                    
                    <msItem n="9" xml:id="Javanese_10_I"> 
                        <locus>Ff. 23r-v.</locus>
                        <note>Fragment mentioning <persName>Prabu Wibisana</persName>.</note>
                        
                    </msItem>
                    
                    <msItem n="10" xml:id="Javanese_10_J">
                        <locus> Ff. 24r.</locus>
                        <note>Unidentified fragment.</note>
                        
                    </msItem>
                    
                    
